<% if @duties.present? %>
  <table class="list">
    <tbody>
      <tr>
        <td>役割</td>
        <td>氏名</td>
      </tr>
      <% @duties.each do |duty| %>
        <tr>
          <td><%= duty.role %></td>
          <td><%= duty.member&.full_name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <% else %>
    <p>記事がありません。</p>
<% end %>